{
  "name": "makehuman.js",
  "version": "0.0.1",
  "description": "An experimental Makehuman port to Typescript/Web/Electron, aiming for better UX, Clean Code and TDD",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "build:watch": "rollup --config rollup.config.js --watch",
    "build:electron": "tsc --outDir dist/electron src/electron/main.ts src/electron/preload.ts",
    "build:directoryfiles": "tsc --outDir dist/filesystem src/filesystem/build-directory-files.ts && node dist/filesystem/build-directory-files.js",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "start": "npm run build && electron ./dist/main.js",
    "test": "mocha -r ts-node/register -r test/bootstrap.ts 'test/**/*.spec.ts'",
    "update": "npx npm-check-updates -u && npm install"
  },
  "author": "Mark-Andr√© Hopf <mhopf@mark13.org> based on work of the MakeHuman Team (www.makehumancommunity.org)",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "electron": "^16.0.6",
    "toad.js": "0.0.21",
    "toad.jsx": "^0.0.3"
  },
  "devDependencies": {
    "@electron/remote": "^2.0.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/chai": "^4.3.0",
    "@types/chai-string": "^1.4.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "chai": "^4.3.4",
    "chai-string": "^1.5.0",
    "eslint": "^8.6.0",
    "gl-matrix": "^3.4.3",
    "mocha": "^9.1.3",
    "rollup": "^2.63.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
