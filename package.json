{
  "name": "makehuman.js",
  "version": "0.0.1",
  "description": "An experimental Makehuman port to Typescript/Web/Electron, aiming for better UX, Clean Code and TDD",
  "scripts": {
    "build": "tsc --outDir dist src/main.ts src/preload.ts && webpack",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "test": "mocha -r ts-node/register -r test/bootstrap.ts 'test/**/*.spec.ts'"
  },
  "author": "",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "electron": "^10.1.1",
    "toad.js": "0.0.10"
  },
  "devDependencies": {
    "@electron/remote": "^1.0.0",
    "@types/chai": "^4.2.12",
    "@types/chai-string": "^1.4.2",
    "@types/electron": "^1.6.10",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.10.1",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "gl-matrix": "^3.3.0",
    "mocha": "^8.1.3",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
